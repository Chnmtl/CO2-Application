<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
			xmlns:app="http://schemas.android.com/apk/res-auto"
			xmlns:tools="http://schemas.android.com/tools"
			android:id="@+id/mobile_navigation"
			app:startDestination="@+id/navigation_home">

	<fragment
			android:id="@+id/navigation_camera"
			android:name="com.example.clear_co2_application.ui.dashboard.DashboardFragment"
			android:label="@string/title_camera"
			tools:layout="@layout/fragment_dashboard"/>
	<fragment
			android:id="@+id/navigation_profile"
			android:name="com.example.clear_co2_application.ui.home.HomeFragment"
			android:label="@string/title_profile"
			tools:layout="@layout/fragment_home"/>
	<fragment
			android:id="@+id/navigation_home"
			android:name="com.example.clear_co2_application.ui.home.HomeFragment"
			android:label="@string/title_home"
			tools:layout="@layout/fragment_home"/>

	<fragment
			android:id="@+id/navigation_statistics"
			android:name="com.example.clear_co2_application.ui.dashboard.DashboardFragment"
			android:label="@string/title_statistics"
			tools:layout="@layout/fragment_dashboard"/>

	<fragment
			android:id="@+id/navigation_list"
			android:name="com.example.clear_co2_application.ui.notifications.NotificationsFragment"
			android:label="@string/title_list"
			tools:layout="@layout/fragment_notifications"/>
</navigation>