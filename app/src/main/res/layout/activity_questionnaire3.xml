<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".quiz.Questionnaire3_Activity">

	<TextView
			android:text="What is your base year?"
			android:layout_width="400dp"
			android:layout_height="83dp" android:id="@+id/textView12"
			android:textColor="#707070"
			android:fontFamily="@font/open_sans_bold" android:textSize="24sp"
			android:foregroundGravity="center" android:gravity="center" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="110dp"/>
	<Button
			android:text="Continue"
			android:layout_width="338dp"
			android:layout_height="60dp" android:id="@+id/year_continue_BUTTON"
			app:backgroundTint="#707079"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/_dynamic" app:layout_constraintHorizontal_bias="0.493"
			app:layout_constraintVertical_bias="0.34"/>
	<Spinner
			android:layout_width="145dp"
			android:layout_height="46dp" android:id="@+id/years_SPINNER"
			app:layout_constraintTop_toBottomOf="@+id/textView12"
			android:layout_marginTop="24dp" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent" android:gravity="center" android:foregroundGravity="center"/>
</androidx.constraintlayout.widget.ConstraintLayout>