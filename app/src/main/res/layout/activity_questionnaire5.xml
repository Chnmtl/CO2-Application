<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".quiz.Questionnaire5_Activity">

	<ListView
			android:layout_width="303dp"
			android:layout_height="416dp"
			android:id="@+id/needs_LIST"
			app:layout_constraintTop_toBottomOf="@+id/textView14"
			app:layout_constraintBottom_toTopOf="@+id/last_continue_BUTTON" app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="24dp"/>
	<TextView
			android:text="Why do you want to calculate your carbon footprint?"
			android:layout_width="400dp"
			android:layout_height="83dp" android:id="@+id/textView14"
			android:textColor="#707070"
			android:fontFamily="@font/open_sans_bold" android:textSize="24sp"
			android:foregroundGravity="center" android:gravity="center" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="108dp"
			app:layout_constraintHorizontal_bias="0.454"/>
	<Button
			android:text="Continue"
			android:layout_width="338dp"
			android:layout_height="60dp" android:id="@+id/last_continue_BUTTON"
			app:backgroundTint="#707079"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/_dynamic" app:layout_constraintHorizontal_bias="0.479"
			app:layout_constraintVertical_bias="0.34"/>
</androidx.constraintlayout.widget.ConstraintLayout>