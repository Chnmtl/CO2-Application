<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".quiz.Questionnaire1_Activity">


	<TextView
			android:text="Which country is your company based in?"
			android:layout_width="400dp"
			android:layout_height="83dp" android:id="@+id/textView11"
			android:textColor="#707070"
			android:fontFamily="@font/open_sans_bold" android:textSize="24sp"
			android:foregroundGravity="center" android:gravity="center" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent"
			app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp"/>
	<Button
			android:text="Continue"
			android:layout_width="338dp"
			android:layout_height="60dp" android:id="@+id/country_continue_BUTTON"
			app:backgroundTint="#707079"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintTop_toBottomOf="@+id/_dynamic" app:layout_constraintHorizontal_bias="0.493"
			app:layout_constraintVertical_bias="0.34"/>
	<ListView
			android:layout_width="410dp"
			android:layout_height="450dp"
			app:layout_constraintTop_toBottomOf="@+id/textView11" app:layout_constraintStart_toStartOf="parent"
			android:layout_marginEnd="1dp" android:layout_marginRight="1dp" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginBottom="18dp" app:layout_constraintBottom_toTopOf="@+id/country_continue_BUTTON"
			android:id="@+id/countries_LIST"/>
</androidx.constraintlayout.widget.ConstraintLayout>