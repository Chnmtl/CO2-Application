<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		tools:context=".quiz.QuestionnaireStartScreen">

	<Button
			android:text="Start the questionnaire"
			android:layout_width="338dp"
			android:layout_height="60dp" android:id="@+id/button2"
			app:backgroundTint="#707079"
			app:layout_constraintTop_toBottomOf="@+id/textView6"
			android:layout_marginTop="32dp" android:layout_marginEnd="37dp" android:layout_marginRight="37dp"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
			android:layout_marginLeft="37dp" android:layout_marginStart="37dp"/>
	<TextView
			android:text="We want to know more about you! Please answer these few questions before kickstarting."
			android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/textView6"
			android:textColor="#707070"
			android:fontFamily="@font/open_sans_bold" android:textSize="24sp"
			android:foregroundGravity="center" android:gravity="center" app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
			android:layout_marginTop="250dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>